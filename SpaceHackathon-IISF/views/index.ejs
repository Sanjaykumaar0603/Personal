      <script>
        document.addEventListener('DOMContentLoaded', function () {
          const barChartData = {
            labels: ["18-DEC", "19-DEC", "20-DEC", "21-DEC", "22-DEC", "23-DEC", "24-DEC"],
            datasets: [{
              label: 'Daily Registered Users',
              data: [166, 190, 209, 152, 158, 138, 81],
              backgroundColor: ['#007bff', '#17a2b8', '#007bff', '#17a2b8', '#007bff', '#17a2b8', '#007bff'],
              hoverBackgroundColor: ['#007bff', '#17a2b8', '#007bff', '#17a2b8', '#007bff', '#17a2b8', '#007bff'],
            }]
          };

          const lineChartData = {
            labels: ["MAY-23", "JUN-23", "JUL-23", "AUG-23", "SEP-23", "OCT-23"],
            datasets: [{
              label: 'Monthly Hits (Millions)',
              data: [836.72, 892.922, 916.144, 891.868, 868, 833],
              borderColor: '#007bff',
              backgroundColor: 'rgba(0, 123, 255, 0.1)',
              pointBackgroundColor: '#007bff',
              pointRadius: 5,
              pointHoverRadius: 7,
            }]
          };

          const barOptions = {
            scales: {
              x: {
                type: 'category',
                title: {
                  display: true,
                  text: 'Date'
                }
              },
              y: {
                beginAtZero: false,
                min: 80,
                title: {
                  display: true,
                  text: 'Number of Users'
                }
              }
            },
            plugins: {
              tooltip: {
                callbacks: {
                  label: (context) => {
                    const label = context.dataset.label || '';
                    const value = context.parsed.y;
                    return `${label}: ${value}`;
                  }
                }
              }
            }
          };

          const lineOptions = {
            scales: {
              x: {
                type: 'category',
                title: {
                  display: true,
                  text: 'Months'
                }
              },
              y: {
                beginAtZero: true,
                min: 830,
                title: {
                  display: true,
                  text: 'Monthly Hits (In Millions)'
                }
              }
            },
            plugins: {
              tooltip: {
                callbacks: {
                  label: (context) => {
                    const label = context.dataset.label || '';
                    const value = context.parsed.y;
                    return `${label}: ${value}M`;
                  }
                }
              }
            }
          };

          const barCtx = document.getElementById('barChart').getContext('2d');
          const barChart = new Chart(barCtx, {
            type: 'bar',
            data: barChartData,
            options: barOptions
          });

          const lineCtx = document.getElementById('lineChart').getContext('2d');
          const lineChart = new Chart(lineCtx, {
            type: 'line',
            data: lineChartData,
            options: lineOptions
          });
        });
      </script>
